generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PlayerGame {
  id        Int       @id @default(autoincrement())
  playerId  Int
  gameId    Int
  createdAt DateTime  @default(now())

  @@unique([playerId, gameId])
}
